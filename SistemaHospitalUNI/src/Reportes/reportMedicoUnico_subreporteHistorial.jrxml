	<queryString>
		<![CDATA[SELECT
     cita.id_cita,
     cita.id_medico,
     cita.id_paciente,
     paciente.nombre,
     paciente.apellido,
     DATE_FORMAT(cita.fecha,'%d-%m-%Y'),
     time(cita.hora),
     paciente.estado,
     cita.estado
FROM Cita cita
Inner Join Paciente paciente
On paciente.id_paciente = cita.id_paciente
Where cita.id_medico = $P{parametroIDMed}]]>
